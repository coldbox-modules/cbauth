{
    "name":"cbauth",
    "version":"4.1.2",
    "location":"forgeboxStorage",
    "author":[
        "Eric Peterson",
        "Mike Burt"
    ],
    "repository":{
        "type":"git",
        "URL":"https://github.com/coldbox-modules/cbauth"
    },
    "slug":"cbauth",
    "shortDescription":"Helpers for authentication in ColdBox",
    "type":"modules",
    "keywords":[
        "authentication",
        "sessions"
    ],
    "private":false,
    "license":[
        {
            "type":"MIT",
            "URL":"https://raw.githubusercontent.com/coldbox-modules/cbauth/master/LICENSE"
        }
    ],
    "dependencies":{
        "cbstorages":"^2.0.0"
    },
    "devDependencies":{
        "testbox":"^3.0.0"
    },
    "installPaths":{
        "testbox":"testbox/",
        "cbstorages":"modules/cbstorages/"
    },
    "scripts":{
        "generateAPIDocs":"rm .tmp --recurse --force && docbox generate mapping=cbauth excludes=tests|ModuleConfig strategy-outputDir=.tmp/apidocs strategy-projectTitle=cbauth",
        "commitAPIDocs":"run-script generateAPIDocs && !git add docs/apidocs/* && !git commit -m 'Updated API Docs'"
    },
    "ignore":[
        "**/.*",
        "test",
        "tests",
        "docs/**/*.*",
        "server.json"
    ]
}
